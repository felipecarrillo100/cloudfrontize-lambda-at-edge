{
  "name": "cloudfrontize-lambda-at-edge",
  "description": "Simulate your Lambda@Edge functions locally for on a CloudFront-like static server.",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/cli.js",
  "bin": {
    "cloudfrontize": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "prepublishOnly": "npm run build",
    "test": "jest --forceExit",
    "test:local": "node ./bin/cli.js -C ./www -e ./samples/medium/lambda-edge-authorization.js -l 5174",
    "test:dist": "node ./dist/cli.js -C ./www -e ./samples/medium/lambda-edge-authorization.js -l 5174"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "compression": "^1.7.4",
    "serve-handler": "^6.1.6"
  },
  "devDependencies": {
    "esbuild": "^0.27.3",
    "jest": "^30.2.0"
  },
  "keywords": [
    "cloudfront",
    "aws",
    "lambda-at-edge",
    "compression",
    "brotli",
    "static-server"
  ],
  "author": "Felipe Carrillo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/felipecarrillo/cloudfrontize-lambda-at-edge.git"
  }
}
